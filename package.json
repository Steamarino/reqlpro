{
  "name": "reqlpro",
  "productName": "ReQLPro",
  "version": "0.0.2",
  "engine": "node 5.9.1",
  "description": "A simple desktop client to connect to RethinkDB servers to work with data.",
  "main": "main.js",
  "scripts": {
    "start": "node tools/start.js",
    "test": "mocha",
    "test-coverage": "npm test && istanbul cover node_modules/mocha/bin/_mocha -- -R spec",
    "electron": "electron main.js",
    "watch": "webpack --colors --watch",
    "build": "electron-packager . ReQLPro --platform=darwin --arch=x64 && cp ./public/reqlpro.icns ReQLPro-darwin-x64/ReQLPro.app/Contents/Resources/atom.icns",
    "build-dmg": "node tools/build-dmg.js",
    "icon": "cp ./public/icon.icns ReQLPro-darwin-x64/ReQLPro.app/Contents/Resources/atom.icns",
    "package": "asar pack ReQLPro-darwin-x64/ReQLPro.app/Contents/Resources/app ReQLPro-darwin-x64/ReQLPro.app/Contents/Resources/app.asar"
  },
  "repository": {},
  "keywords": [
    "RethinkDB"
  ],
  "author": "CodeHangar, Inc",
  "license": "",
  "bugs": {},
  "homepage": "http://codehangar.io",
  "devDependencies": {
    "appdmg": "^0.4.0",
    "asar": "^0.10.0",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "brace": "^0.8.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "electron-packager": "^6.0.0",
    "electron-prebuilt": "^0.36.0",
    "electron-rebuild": "^1.1.3",
    "file-loader": "^0.8.5",
    "istanbul": "^0.4.4",
    "jdenticon": "^1.3.2",
    "md5": "^2.1.0",
    "mocha": "^2.5.3",
    "mockery": "^1.7.0",
    "node-sass": "^3.4.2",
    "riek": "^1.0.0",
    "sass-loader": "^3.2.0",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9"
  },
  "dependencies": {
    "analytics-node": "^2.1.0",
    "bootstrap-sass": "^3.3.6",
    "classnames": "^2.2.3",
    "co": "^4.6.0",
    "fixed-data-table": "^0.6.0",
    "font-awesome": "^4.5.0",
    "node-uuid": "^1.4.7",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-json-tree": "^0.6.3",
    "rethinkdb": "^2.2.2"
  }
}
